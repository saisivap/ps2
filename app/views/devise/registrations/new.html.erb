<!--<h2>Sign up</h2>-->
<h2 class="ui center aligned icon header">
  <!--  <i class="circular users icon"></i>-->
  <p> Welcome to TimeOut AIT Cafe</p>
  <i class="inverted orange circular power off icon"></i>
  Sign Up to login and continue
</h2>
<div class="ui form">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
    <div class="field">
      <%= f.label :first_name,"First Name" %><br />
      <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
    </div>
    <div class="field">
      <%= f.label :last_name,"Last Name" %><br />
      <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
    </div>
    <div class="field">
      <%= f.label :stID,"AIT Student ID " %><br />
      <%= f.text_field :stID, autofocus: true, autocomplete: "stID" %>
    </div>
    <div class="field">
      <%= f.label :phone_number,"Phone Number " %><br />
      <%= f.text_field :phone_number, autofocus: true, autocomplete: "stID" %>
    </div>
    <div class="field">
      <%= f.label :country,"Country" %><br />
      <%= f.text_field :country, autofocus: true, autocomplete: "stID" %>
    </div>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="field">
      <%= f.label :password %>
      <% if @minimum_password_length %>
        <em>(<%= @minimum_password_length %> characters minimum)</em>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "new-password" %>
    </div>

    <div class="field">
      <%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
    </div>

    <div class="actions">
      <%= f.submit "Sign up",class: "ui button" %>
    </div>
  <% end %>
</div>
<%= render "devise/shared/links" %>

